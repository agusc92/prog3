package arboles;

public class Tree {
	private Node raiz;
	
	public Tree() {
		this.raiz = null;
	}
	
	public int getRaiz() {
		
		return this.raiz.getKey();
	}
	
	public boolean isEmpty() {
		return this.raiz == null;
			
	}
	
	public void add(int key) {
		if(this.raiz==null)
			this.raiz = new Node(key);
		
	}
}
